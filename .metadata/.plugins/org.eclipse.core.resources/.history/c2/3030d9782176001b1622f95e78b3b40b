		//System.out.println(e);
		
		int f = 4;
		f++;
		//System.out.println(f);
		++f;
		//System.out.println(f);
		f = f + 1;
		//System.out.println(f);
		
		int g = 5;
		int h = 8;
		int sum = g++ + h;
		System.out.println(sum);
		
		
		
		
		
		
		
