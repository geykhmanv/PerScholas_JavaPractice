package com.perscholas.java_basics;

public class Loops {

	public static void prompt1() {
		for(int i = 1; i <= 10; i++) {
			System.out.println(i);
		}//for
	}//prompt1
	
	public static void prompt2() {
		int i = 0;
		while(i <= 100) {
			System.out.println(i);
			i += 10;
		}//while
	}//prompt2
	
	public static void prompt3() {
		int i = 1;
		do {
			System.out.println(i);
		}while(i <= 10);
	}//prompt3
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//prompt1();
		//prompt2();
		//prompt3();
		
		
	}//public static void main(String[] args)

}//public class Loops 
