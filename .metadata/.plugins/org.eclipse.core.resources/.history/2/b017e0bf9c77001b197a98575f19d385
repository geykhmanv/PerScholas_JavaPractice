package com.perscholas.cafe;

public class Product {

	private String name;
	private double price;
	private String description;
	private int quantity;
	
	public Product() {
		
	}//no arg constructor
	
	public Product(String name, double price, String description) {
		this.name = name;
		this.price = price;
		this.description = description;
	}//constructor
	
	
}//public class Product
